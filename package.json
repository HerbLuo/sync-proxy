{
  "name": "@o2v/sync-proxy",
  "version": "0.0.1-beta.8",
  "description": "[sync-proxy] let you write async task chains using synchronous syntax.",
  "main": "lib/index.js",
  "typings": "typings/index.d.ts",
  "files": [
    "lib",
    "typings"
  ],
  "scripts": {
    "test": "mocha -r ts-node/register test/**/*.ts",
