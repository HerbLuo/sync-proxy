    "mocha": "^5.2.0",
    "rimraf": "^2.6.2",
    "rollup": "^0.66.2",
    "rollup-plugin-typescript": "^1.0.0",
    "rollup-plugin-uglify": "^6.0.0",
    "ts-node": "^7.0.1",
    "typescript": "^3.1.1",
    "uglify-es": "^3.3.9"
  },
  "dependencies": {
    "tslib": "^1.9.3"
  },
  "keywords": [
    "promise",
